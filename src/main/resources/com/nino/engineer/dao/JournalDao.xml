<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nino.engineer.dao.JournalDao">

    <insert id="addProjectLogInfo" parameterType="com.nino.engineer.domain.Journal" flushCache="true">
    insert into
    journal(u_id,p_id,p_name,j_Time,j_week,j_weather,j_ambient,j_work,j_Enter,j_inspection,j_patrol,j_parallel,j_reply,j_rectification,j_unanswered,j_other)
    values
    (
    #{u_id},
    #{p_id},
    #{p_name},
    #{j_Time},
    #{j_week},
    #{j_weather},
    #{j_ambient},
    #{j_work},
    #{j_Enter},
    #{j_inspection},
    #{j_patrol},
    #{j_parallel},
    #{j_reply},
    #{j_rectification},
    #{j_unanswered},
    #{j_other}
    )
</insert>


    <!-- 根据时间查找日志表信息 -->
    <select id="findLogTableInfo" resultType="com.nino.engineer.domain.Journal">
select id,u_id,p_id,p_name,j_Time,j_week,j_weather,j_ambient,j_work,j_Enter,j_inspection,j_patrol,j_parallel,j_reply,j_rectification,j_unanswered,j_other
from journal
where j_time = #{_parameter}
</select>
</mapper>